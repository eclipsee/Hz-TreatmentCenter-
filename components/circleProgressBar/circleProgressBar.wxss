.score-rate-wrapper {
  position: relative;
  height: 140rpx;
  width: 140rpx;
  display: inline-block;
}
.score-rate-wrapper .progress-bg {
  position: absolute;
  width: 100%;
  height: 100%;
  border-radius: 100%;
  background-color: #ededed;
}
.score-rate-wrapper .score-rate {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  margin-top: 50%;
  margin-left: 50%;
  top: -50rpx;
  left: -50rpx;
  border-radius: 100%;
  background-color: #fff;
}
.score-rate-wrapper .score-rate text {
  width: 100%;
  display: inline-block;
  line-height: 100rpx;
  color: #f86442;
  font-weight: bold;
  font-size: 28rpx;
  text-align: center;
}
canvas {
  position: absolute;
  z-index: 100;
}
