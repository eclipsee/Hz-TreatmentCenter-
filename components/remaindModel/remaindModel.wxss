/* components/remaindModel/remaindModel.wxss */
.remaindModelShadow{
  width:100%;
  height:100%;
  position:fixed;
  top:0;
  left:0;
  background:rgba(0,0,0,0.7);
  z-index:1000;
}
.remaindModel{
  width:590rpx;
  height:620rpx;
  background:rgba(255,255,255,1);
  border-radius:16rpx;
  margin-top:400rpx;
  margin-left:80rpx;
  overflow: hidden;
}
.topImg{
  width:590rpx;
height:200rpx;
}
.remaindTxt{
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  text-align: center;
  margin-top: 36rpx;
}
.remaindTxt view{
  line-height: 50rpx;
}
.openBtn{
  width:496rpx;
  height:80rpx;
  line-height: 80rpx;
  background:rgba(248,100,66,1);
  border-radius:47rpx;
  margin:50rpx 47rpx 44rpx;
  font-size:32rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  text-align: center;
}
.cancel{
  font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(153,153,153,1);
text-align: center;
}