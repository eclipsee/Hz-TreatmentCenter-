view{
  box-sizing: border-box;
}
.content-wrapper {
  padding-bottom: 110rpx;
}
.content-wrapper .image-box {
  /* display: block; */
  width:200rpx;
  margin:30rpx auto 0;
  height:200rpx;
}
.content-wrapper .audio-box {
  margin: 30rpx  0;
  width: 400rpx;
}
.content-wrapper .video-box {
  width: 524rpx;
  margin-top:30rpx;
  /* margin-left:110rpx; */
}
.content-wrapper .answer-wrapper {
  padding: 58rpx 40rpx 40rpx 40rpx;
  box-sizing: border-box;
  background: #fff;
}
.content-wrapper .answer-wrapper .score-wrapper {
  position: relative;
  height:160rpx;
}
.content-wrapper .answer-wrapper .score-wrapper .score-title-wrapper {
  display: inline-block;
}
.content-wrapper .answer-wrapper .score-wrapper .score-title-wrapper text {
  display: block;
  font-weight: 500;
}
.content-wrapper .answer-wrapper .score-wrapper .score-title-wrapper text:nth-child(1) {
  font-size: 40rpx;
}
.content-wrapper .answer-wrapper .score-wrapper .score-title-wrapper text:nth-child(2) {
  font-size: 38rpx;
  color: #cccccc;
  margin-top: 10rpx;
}
.content-wrapper .answer-wrapper .score-wrapper .score-rate-wrapper {
  position: absolute;
  right: 0;
  top: -3rpx;
}
.content-wrapper .answer-wrapper .answer-collec-wrapper {
  margin-top: 40rpx;
  border-top: 1rpx solid #ededed;
  padding-top: 46rpx;
  box-sizing: border-box;
}
.content-wrapper .answer-wrapper .answer-collec-wrapper .answer-collec {
  display: flex;
  flex-flow: wrap;
}
.content-wrapper .answer-wrapper .answer-collec-wrapper .answer-collec .answer {
  width: 140rpx;
  margin-bottom: 24rpx;
  margin-right: 36rpx;
}
.content-wrapper .answer-wrapper .answer-collec-wrapper .answer-collec .answer:nth-child(4n) {
  margin-right: 0;
}
.content-wrapper .answer-wrapper .answer-collec-wrapper .answer-collec .answer .tit-no {
  width: 100%;
  height: 54rpx;
  border-radius: 36rpx;
  border: 2rpx solid #e8e8e8;
}
.content-wrapper .answer-wrapper .answer-collec-wrapper .answer-collec .answer .active {
  width: 100%;
  height: 54rpx;
  background:rgba(254,232,227,1);
  border-radius:36rpx;
  border:1px solid rgba(248,100,66,0.2);
}
.content-wrapper .answer-wrapper .answer-collec-wrapper .answer-collec .answer .active text{
  color:rgba(248,100,66,1)!important;
}
.content-wrapper .answer-wrapper .answer-collec-wrapper .answer-collec .answer .tit-no text {
  color: #666666;
  font-size: 26rpx;
  display: inline-block;
  vertical-align: top;
  /* height:56rpx; */
  margin-top:7rpx;
  /* line-height: 56rpx; */
  /* vertical-align: middle; */
  width: 100%;
  text-align: center;
}
.content-wrapper .answer-wrapper .answer-collec-wrapper .answer-collec .answer image {
  margin: 10rpx auto 0 auto;
  display: block;
  width: 36rpx;
  height: 36rpx;
}
.to-get-all {
  margin: 0 auto;
  width: 180rpx;
  margin-top:30rpx;
}
 .to-get-all image{
  vertical-align: middle;
}
.to-get-all .less{
  transform: rotate(180deg);
}
 .to-get-all text {
  font-size: 26rpx;
  color: #999999;
  vertical-align: middle;
}
.to-get-all image {
  display: inline-block;
  margin-left: 16rpx;
  width: 20rpx;
  height: 12rpx;
}
.to-get-all .to-get-all-Txt{
  display: inline-block!important;
  font-size: 26rpx;
  color: #999999;
  vertical-align: middle;
  margin-left:25rpx;
}
.content-wrapper .divide-line {
  height: 20rpx;
  background-color: #f0f2f5;
}
.content-wrapper .analysis-wrapper {
  padding: 40rpx 30rpx 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.content-wrapper .analysis-wrapper text {
  display: block;
}
.content-wrapper .analysis-wrapper .analysis-title {
  font-size: 36rpx;
  font-weight: 500;
}
.content-wrapper .analysis-wrapper .title-no-and-type {
  font-size: 28rpx;
  color: #666666;
  margin-top: 40rpx;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper {
  background-color: #f9fafb;
  margin-top: 30rpx;
  border-radius: 12rpx;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail {
  padding: 30rpx 30rpx 30rpx 30rpx;
  box-sizing: border-box;
}

.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .question-instruction .question-title {
  font-size: 32rpx;
  line-height: 42rpx;
  color: #836F66;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .question-instructionSub{
  /* height:392rpx;
  overflow: hidden; */
}
.question-title {
  font-size: 32rpx;
  line-height: 56rpx;
  color: #666;
  margin-bottom: 40rpx;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .option {
  width: 100%;
  margin-top: 40rpx;
  box-sizing: border-box;
  background-color: #f9fafb;
  border-radius: 12rpx;
  color: #999999;
  font-size: 32rpx;
  padding-bottom: 40rpx;
}
.analysis-comment{
  width:650rpx;
  margin:0 auto;
  box-sizing: border-box;
  background:#FFF7F0;
  padding: 24rpx;
  border:1rpx solid #F6D8B9;
  border-radius:6rpx;
  position: relative;
}
.analysis-comment .trangle{
  position: absolute;
  top:-10rpx;
  left:45rpx;
  width:16rpx;
  height:16rpx;
  transform:rotate(45deg);
  background:#FFF7F0;
  border-top:1px solid #F6D8B9;
  border-left:1px solid #F6D8B9;

}
.analysis-comment .spreadContent{
  /* height:600rpx;
  overflow: hidden; */
}

.analysis-comment .commentHeadImg{
  width:65rpx;
  height:65rpx;
  border-radius:33rpx;
  display: inline-block;
}
.analysis-comment .commentNickname{
  color:#4D423D;
  display: inline-block;
  font-size: 30rpx;
  font-weight: 500;
  margin-left: 20rpx;
  position:relative;
  top:-22rpx;
}
.audioItem .audio-box{
  margin:30rpx 0;
}
.answerItem{
  display: inline-block;
  width:200rpx;
  height:200rpx;
  /* background-color: bisque; */
  position: relative;
  margin:20rpx 15rpx 20rpx 0;
  overflow: hidden;
}
.answerItem .answer-image{
  width:200rpx;
  height:200rpx;
}
.videoPreview{
  width:200rpx;
  height:200rpx;
  position:relative;
  top:-20rpx;
}
.answer-image .video-wrapper{
  width:200rpx;
  height:200rpx;
}
.videoItem{
  display: inline-block;
  width:200rpx;
  height:200rpx;
  /* background-color: bisque; */
  margin-top:20rpx;
}
.videoImg{
  width:200rpx;
  height:200rpx;
  position:relative;
top:-20rpx;

}

.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .option:last-child{
  padding-bottom: 0rpx;
}

.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .option .option-header {
  width: 100%;
  margin-bottom: 30rpx;
  position: relative;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .option .option-header text {
  display: inline;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .option .option-header .result-check {
  position: absolute;
  right: 0;
  top: 0;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .option .option-header .result-check image:nth-child(1) {
  display: inlin-block;
  margin-right: 24rpx;
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .option .option-header .result-check image:nth-child(2) {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .answer-detail .option .option-content {
  width: 100%;
  color: #333;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .analysis-answer {
  padding: 30rpx;
  border-top: 1rpx solid #EDEDED;
}
.detail-title {
  font-size: 28rpx;
  font-weight: bold;
  display: inline-block!important;
  margin-bottom:20rpx;
}
 .analysis-answer .detail-titleEssence{
   display: inline-block!important;
   width:88rpx;
   height:40rpx;
   text-align: center;
   line-height: 40rpx;
   background:linear-gradient(297deg,rgba(252,208,119,1) 0%,rgba(248,100,66,1) 100%);
  border-radius:22rpx;
  color:#fff;
  font-size: 24rpx;
  margin-left: 12rpx;
}
.content-wrapper .analysis-wrapper .analysis-content-wrapper .analysis-detail .detail-content {
  width:100%;
  color: #999999;
  font-size: 30rpx;
  line-height: 42rpx;
  margin-top: 20rpx;
}
.analysis-detail{
  padding:30rpx;
